{
    "name": "ivqonsanada/phive",
    "description": "A kind of freelancing website for college.",
    "keywords": ["spa", "laravel", "vue"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "fideloper/proxy": "4.4.1",
        "fruitcake/laravel-cors": "1.0.6",
        "guzzlehttp/guzzle": "6.5.5",
        "laravel/framework": "7.30.5",
        "laravel/socialite": "4.4.1",
        "laravel/tinker": "2.6.2",
        "laravel/ui": "2.5.0",
        "pusher/pusher-php-server": "4.1.5",
        "stevebauman/purify": "3.0.3",
        "tymon/jwt-auth": "1.0.2"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "1.7.0",
        "doctrine/dbal": "2.13.5",
        "facade/ignition": "2.16.1",
        "fzaninotto/faker": "1.9.2",
        "laravel/dusk": "6.19.2",
        "mockery/mockery": "1.4.4",
        "nunomaduro/collision": "4.3.0",
        "phpunit/phpunit": "8.5.21"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
              "laravel/dusk"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php artisan jwt:secret --force --ansi"
        ]
    }
}
